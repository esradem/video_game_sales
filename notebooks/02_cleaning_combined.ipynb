   "source": [
    "from fuzzywuzzy import fuzz\n",
    "\n",
    "def fuzzy_grouping(names, threshold=90):\n",
    "    grouped = []\n",
    "    used = set()\n",
    "\n",
    "    for name in names:\n",
    "        if name in used:\n",
    "            continue\n",
    "        group = [other for other in names \n",
    "                 if fuzz.token_sort_ratio(name, other) >= threshold]\n",
    "        grouped.append((name, group))\n",
    "        used.update(group)\n",
    "    \n",
    "    return grouped\n"
